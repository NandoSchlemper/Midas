{
  "name": "@org/api-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@org/api"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "packages/apps/api-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@org/api:build",
          "@org/api:serve"
        ]
      }
    }
  },
  "devDependencies": {
    "@types/node": "^20.19.9"
  }
}
